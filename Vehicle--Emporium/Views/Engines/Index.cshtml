@model IEnumerable<Vehicle__Emporium.Models.Engine>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.vehicleID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.engineMake)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.engineModel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.enginePower)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.engineType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.engineHour)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.vehicleID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.engineMake)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.engineModel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.enginePower)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.engineType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.engineHour)
            </td>
        </tr>
}
    </tbody>
</table>
